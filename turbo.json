{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_APP_URL",
    "ANALYZE",
    "NAVIGATION_APP_URL",
    "FUMA_MDX_URL",
    "BETTER_AUTH_SECRET",
    "BETTER_AUTH_URL",
    "VERCEL_CLIENT_ID",
    "VERCEL_CLIENT_SECRET",
    "DATABASE_URL",
    "DATABASE_AUTH_TOKEN"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "analyze": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "dev:inspect": {
      "cache": false,
      "persistent": true
    },
    "change": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "version": {
      "cache": false
    },
    "release": {
      "dependsOn": ["build"],
      "cache": false
    },
    "//#check": {},
    "//#fix": {
      "cache": false
    },
    "//#lint": {},
    "//#format": {
      "cache": false
    },
    "//#doctor": {
      "cache": false
    },
    "//#devtools": {
      "cache": false,
      "persistent": true
    }
  }
}
