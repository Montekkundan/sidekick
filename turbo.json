{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "analyze": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "dev:inspect": {
      "cache": false,
      "persistent": true
    },
    "//#check": {},
    "//#fix": {
      "cache": false
    },
    "//#lint": {},
    "//#format": {
      "cache": false
    },
    "//#doctor": {
      "cache": false
    },
    "//#devtools": {
      "cache": false,
      "persistent": true
    }
  }
}
